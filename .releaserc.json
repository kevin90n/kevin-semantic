{
    "tagFormat": "${version}",
    "branches": [
        "main",
        {
            "name": "beta1",
            "prerelease": true
        },
        {
            "name": "alpha1",
            "prerelease": true
        }
    ],
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        "@semantic-release/github"
    ],
    "publish": false
}
